{
  "manifest_version": 3,
  "name": "s-cookie",
  "version": "2.0.2",
  "description": "s-cookie",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_title": "s-cookie",
    "default_popup": "./index.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "contextMenus"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "devtools_page": "./index.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "pageScript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}