{
  "manifest_version": 2,
  "name": "s-cookie",
  "version": "2.0.1",
  "description": "s-cookie",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "s-cookie",
    "default_popup": "./index.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content.js"
      ]
    }
  ]
}